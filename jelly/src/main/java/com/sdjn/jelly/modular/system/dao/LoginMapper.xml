<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdjn.jelly.modular.system.dao.LoginDao">
    <resultMap id="BaseResultMap" type="com.sdjn.jelly.modular.system.entity.User">
        <id column="id" property="id" />
        <result column="username" property="username" />
        <result column="password" property="password" />
    </resultMap>

    <select id="accountLogin" resultType="java.lang.Integer">
        SELECT count(*) from sys_user where username = #{username} and password = #{password}
    </select>
</mapper>
